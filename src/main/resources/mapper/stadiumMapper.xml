<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.cos.finaltest.repository.StadiumRepository">
	<insert id="save">
		INSERT INTO stadium(name, createDate) 
		VALUES(#{name}, now()) 
	</insert>
</mapper>
